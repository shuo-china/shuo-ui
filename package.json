{
  "name": "shuo-ui",
  "private": false,
  "version": "0.0.40",
  "description": "A Component Library for Vue3.js.",
  "keywords": [
    "vue3",
    "components"
  ],
  "author": "zhangjinshuo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shuo-china/shuo-ui"
  },
  "main": "dist/shuo-ui.umd.js",
  "module": "dist/shuo-ui.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/shuo-ui.es.js",
      "require": "./dist/shuo-ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./dist/types/global.d.ts": "./dist/types/global.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build:doc": "vue-tsc --noEmit && vite build --mode doc",
    "build:package": "vue-tsc --noEmit && vite build --mode package",
    "commit": "git-cz"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "async-validator": "^4.2.5",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "highlight.js": "^11.6.0",
    "postcss": "^8.4.14",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typed.js": "^2.0.12",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vue-tsc": "^0.38.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
